{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "AZURE_STORAGE": "UseDevelopmentStorage=true"
  },
  "SessionService": {
    "ApplicationId": "HintKeepApp",
    "Audience": "https://www.hintkeep.net",
    "SigningKey": "A test signing key for local development that has to be of at least a certain size otherwise the algorithm cannot function (preconditions).",
    "SingingAlgorithm": "HmacSha256"
  },
  "SecurityService": {
    "HashAlgorithm": "SHA256",
    "SaltLength": 10,
    "PasswordFormat": "{0}{1}",
    "ActivationTokenLength": 10,
    "ActivationTokenExpirationMinutes": 60
  },
  "EmailService": {
    "SenderEmailAddress": "no-reply@domain.com",
    "SenderDisplayNameAddress": "HintKeep",
    "ServerAddress": "smtp-server",
    "ServerPort": 1234,
    "ServerCredentials": {
      "UserName": "username",
      "Password": "password"
    }
  }
}